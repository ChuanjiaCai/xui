{
  "name": "xui",
  "version": "0.0.1",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "babelify": "^7.3.0",
    "css-loader": "^0.28.7",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.6.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^0.9.0",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^2.25.0",
    "i18n-react": "^0.3.0-ts1",
    "jshint": "latest",
    "jshint-stylish": "^1.0.0",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
    "parallelshell": "latest",
    "pubsub-js": "^1.6.0",
    "react": "^15.6.1",
    "react-bootstrap": "^0.30.10",
    "react-dom": "^15.6.1",
    "react-dropzone": "^3.8.0",
    "react-hot-loader": "^1.3.1",
    "react-lite": "^0.15.35",
    "react-overlays": "^0.6.10",
    "react-router": "^3.0.5",
    "react-router-bootstrap": "^0.23.1",
    "react-select": "^1.0.0-rc.3",
    "riek": "^1.0.7",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "copy-webpack-plugin": "^4.0.1",
    "webpack": "^3.11.0",
    "webpack-git-hash": "^1.0.2",
    "webpack-md5-hash": "0.0.5",
    "whatwg-fetch": "^2.0.3"
  },
  "dependencies": {},
  "scripts": {
    "build:js": "cat src/js/*.js > ../www/assets/js/xui.js",
    "lint": "jshint **.js",
    "livereload": "live-reload --port 9091 ../www/index.html ../www/assets/js/jsx/*.js",
    "wiredep": "wiredep -s index.html",
    "build": " webpack && npm run build:js",
    "githash": "webpack --config webpack.config.githash.js && npm run build:js",
    "watch:js": "watch 'npm run build:js' src/js/",
    "release": "webpack --progress --profile --colors --config webpack.production.config.js && npm run build:js",
    "oldstuff": "echo ------------ support old version of node.js -----------",
    "oldbuild:css": "cat src/css/*.css > ../www/assets/css/xui.css",
    "oldbuild:jsx": "browserify -t [ babelify --presets [ es2015 react ] ] src/jsx/index.js > ../www/assets/js/jsx/index.js",
    "oldrelease:jsx": "NODE_ENV=production browserify -t [ babelify --presets [ es2015 react ] ] src/jsx/index.js | uglifyjs -mc > ../www/assets/js/jsx/index.js",
    "build:html": "echo ok",
    "oldbuild": "npm run oldbuild:css && npm run oldbuild:jsx && npm run build:html",
    "oldrelease": "npm run oldbuild:css && npm run oldrelease:jsx && npm run build:html",
    "build:watch": "parallelshell 'npm run watch:js' 'webpack -w' "
  }
}
